<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" 
	android:id="@+id/adminconfig"
    android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/mon_"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dip"
             android:layout_marginLeft="125.0dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="12.5dip"
                android:gravity="right"
                android:text="@string/mod1"
                android:textSize="10.0sp" />

            <ToggleButton
                android:id="@+id/togglemod"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:background="@drawable/btn_toggle"
                android:disabledAlpha="?android:disabledAlpha"
                android:textOff="@null"
                android:textOn="@null" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="7.0dip"
                android:gravity="right"
                android:text="@string/mod2"
                android:textSize="12.5sp" />
        </LinearLayout>  
        <LinearLayout
            android:id="@+id/county"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center"
                android:text="@string/suoshudiqu"
                android:textSize="12.5sp" />

            <EditText
                android:id="@+id/countyming"
                android:layout_width="90dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:inputType="text"
                android:textSize="12.5sp" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/name"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center"
                android:text="@string/name1"
                android:textSize="12.5sp" />

            <EditText
                android:id="@+id/nameming"
                android:layout_width="90dip"
                android:layout_height="wrap_content"
                android:layout_marginLeft="35dip"
                android:inputType="text"
                android:textSize="12.5sp" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/phone"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center"
                android:text="@string/phone"
                android:textSize="12.5sp" />

            <EditText
                android:id="@+id/phone1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:inputType="phone"
                android:textSize="12.5sp" />
        </LinearLayout>
         <LinearLayout
            android:id="@+id/ad"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center"
                android:text="@string/address"
                android:textSize="12.5sp" />

            <EditText
                android:id="@+id/addres"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:inputType="text"
                android:textSize="12.5sp" />
        </LinearLayout>
         <LinearLayout
            android:id="@+id/mail"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dip"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dip"
                android:gravity="center"
                android:text="@string/mail"
                android:textSize="12.5sp" />

            <EditText
                android:id="@+id/mailming"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dip"
                android:inputType="text"
                android:textSize="12.5sp" />
        </LinearLayout>
         <Button
        android:id="@+id/submit"
        android:layout_width="50dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dip"
        android:layout_marginLeft="100dip"
        android:textSize="12.5sp" 
        android:text="@string/submit" />
    </LinearLayout>